extends core/base.pug

block title 
  title Вопрос-ответ

block content

  include core/header.pug

  <main>

    .new-container
      
      // faq 
      .faq 

        .title Вопрос-ответ

        form(action="#").faq__form  
          .row.gy
            .faq__input 
              .input 
                .input__label Поиск по разделу вопрос-ответ
                .input__input 
                  input(type="text")
                .input__drop 
                  ul.input__list 
                    li Единственный поставщик
                    li Описание объекта закупки
                    li Изменение контракта
                    li Лекарственные препараты
                    li Электронный аукцион
                    li Ограничение допуска
                    li Отклонение заявок
                    li Требования к участникам
                    li Медизделия
                    li План-график
                    li Обоснование НМЦК
                    li Контракт заключение
                    li Условия контракта
                    li Обеспечение исполнения контракта
                    li Реестр контрактов
            .faq__select 
              .input 
                .input__label ФЗ
                .input__select.input__select--icon 
                  select(name="#") 
                    option(value="#") Не важно
                    option(value="#") ФЗ 1
                    option(value="#") ФЗ 2
                  .input__icon 
                    svg(width="13" height="7" viewBox="0 0 13 7" fill="none" xmlns="http://www.w3.org/2000/svg")
                      path(d="M11.314 1L6.157 5.2L1 1" stroke-width="2" stroke-linecap="round" stroke-linejoin="round")
            .faq__date 
              .input 
                .input__label Дата размещения
                .input__input.input__input--icon
                  input(type="text" autocomplete="off")#datepicker
                  .input__icon 
                    img(src="media/icons/calendar.svg", alt="")
            .faq__btn 
              button(type="submit").btn.btn-blue Искать

